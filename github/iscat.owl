    </owl:NamedIndividual>
    


    <!-- http://www.ontologydesignpatterns.org/ont/is/iscat.owl#GROUPS_ARE_CONTAINERS -->

    <owl:NamedIndividual rdf:about="http://www.ontologydesignpatterns.org/ont/is/iscat.owl#GROUPS_ARE_CONTAINERS">
