    <!-- http://www.ontologydesignpatterns.org/ont/is/iscat.owl#GOVERNMENT_S_POSITIVE_TREATMENT_OF_RIGHTS_IS_SUPPORTING_A_PHYSICAL_STRUCTURE -->

    <owl:NamedIndividual rdf:about="http://www.ontologydesignpatterns.org/ont/is/iscat.owl#GOVERNMENT_S_POSITIVE_TREATMENT_OF_RIGHTS_IS_SUPPORTING_A_PHYSICAL_STRUCTURE">
