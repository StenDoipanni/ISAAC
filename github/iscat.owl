    <!-- http://www.ontologydesignpatterns.org/ont/is/iscat.owl#GOVERNMENT_POWER_IS_BODILY_STRENGTH -->

    <owl:NamedIndividual rdf:about="http://www.ontologydesignpatterns.org/ont/is/iscat.owl#GOVERNMENT_POWER_IS_BODILY_STRENGTH">
