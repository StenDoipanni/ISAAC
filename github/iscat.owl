        <hasExample>(English) The burdens of regulation on corporations hold down their profit margins. MetaNet</hasExample>
    </owl:NamedIndividual>
    


    <!-- http://www.ontologydesignpatterns.org/ont/is/iscat.owl#GOVERNMENT_REGULATION_IS_A_PHYSICAL_CONSTRAINT -->

    <owl:NamedIndividual rdf:about="http://www.ontologydesignpatterns.org/ont/is/iscat.owl#GOVERNMENT_REGULATION_IS_A_PHYSICAL_CONSTRAINT">
        <rdf:type rdf:resource="http://www.ontologydesignpatterns.org/ont/is/iscat.owl#ConceptualMetaphor"/>
