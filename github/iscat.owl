    <!-- http://www.ontologydesignpatterns.org/ont/is/iscat.owl#GROUPS_ARE_POSSESSORS -->

    <owl:NamedIndividual rdf:about="http://www.ontologydesignpatterns.org/ont/is/iscat.owl#GROUPS_ARE_POSSESSORS">
