    </owl:NamedIndividual>
    


    <!-- http://www.ontologydesignpatterns.org/ont/is/iscat.owl#GOVERNMENT_S_HARMFUL_TREATMENT_OF_RIGHTS_IS_TRESPASSING -->

    <owl:NamedIndividual rdf:about="http://www.ontologydesignpatterns.org/ont/is/iscat.owl#GOVERNMENT_S_HARMFUL_TREATMENT_OF_RIGHTS_IS_TRESPASSING">
        <rdf:type rdf:resource="http://www.ontologydesignpatterns.org/ont/is/iscat.owl#ConceptualMetaphor"/>
