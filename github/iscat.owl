    </owl:NamedIndividual>
    


    <!-- http://www.ontologydesignpatterns.org/ont/is/iscat.owl#GROUPS_ARE_PART-WHOLE_STRUCTURES -->

    <owl:NamedIndividual rdf:about="http://www.ontologydesignpatterns.org/ont/is/iscat.owl#GROUPS_ARE_PART-WHOLE_STRUCTURES">
