        <hasExample>(English) Our galaxy has at least 100 billion planets. MetaNet</hasExample>
        <hasExample>(English) The Milky Way belongs to a cluster of galaxies. MetaNet</hasExample>
    </owl:NamedIndividual>
    


    <!-- http://www.ontologydesignpatterns.org/ont/is/iscat.owl#GUIDED_ACTION_IS_GUIDED_MOTION -->

    <owl:NamedIndividual rdf:about="http://www.ontologydesignpatterns.org/ont/is/iscat.owl#GUIDED_ACTION_IS_GUIDED_MOTION">
        <rdf:type rdf:resource="http://www.ontologydesignpatterns.org/ont/is/iscat.owl#ConceptualMetaphor"/>
