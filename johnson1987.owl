    </owl:NamedIndividual>
    


    <!-- http://www.ontologydesignpatterns.org/ont/is/johnson87is.owl#Origin -->

    <owl:NamedIndividual rdf:about="http://www.ontologydesignpatterns.org/ont/is/johnson87is.owl#Origin">
