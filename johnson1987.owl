        <bibRef>Johnson M., The Body in the Mind, University of Chicago Press, 1987 chap 2, p.28</bibRef>
    </owl:NamedIndividual>
    


    <!-- http://www.ontologydesignpatterns.org/ont/is/johnson87is.owl#goal -->

    <owl:NamedIndividual rdf:about="http://www.ontologydesignpatterns.org/ont/is/johnson87is.owl#goal">
        <rdf:type rdf:resource="http://www.ontologydesignpatterns.org/ont/is/johnson87is.owl#IS_Component"/>
        <bibRef>Johnson M., The Body in the Mind, University of Chicago Press, 1987 chap 2, p.28</bibRef>
        <rdfs:comment>Called &quot;goal&quot; or also &quot;terminal point&quot;.</rdfs:comment>
        <rdfs:label>&quot;goal&quot;</rdfs:label>
    </owl:NamedIndividual>
    


    <!-- http://www.ontologydesignpatterns.org/ont/is/johnson87is.owl#landmark -->

    <owl:NamedIndividual rdf:about="http://www.ontologydesignpatterns.org/ont/is/johnson87is.owl#landmark">
        <rdf:type rdf:resource="http://www.ontologydesignpatterns.org/ont/is/johnson87is.owl#IS_Component"/>
        <bibRef>Johnson M., The Body in the Mind, University of Chicago Press, 1987 chap 2, p.33</bibRef>
    </owl:NamedIndividual>
    


